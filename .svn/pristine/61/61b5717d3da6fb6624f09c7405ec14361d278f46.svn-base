*{
	margin: 0rem;
	padding: 0rem;
}
html{
	background-color: #F1F5F8;
}
/*页面上部分*/
#top{
	width: 100%;
	height: 5rem;
	background-color: #FB74A4;
}
/*日统计盒子*/
#cen1{
	width: 90%;
	height: 19.75rem;
	background-color: #FEFEFE;
	margin: 0 auto;
	margin-top: 1.25rem;
	border-radius: 1.25rem;
}
/*月统计盒子*/
#cen2{
	width: 90%;
	height: 19.75rem;
	background-color: #FEFEFE;
	margin: 0 auto;
	margin-top: 1.5rem;
	border-radius: 1.25rem;
}
/*分类统计和月统计的盒子*/
.cen_top{
	width: 90%;
	height: 4.25rem;
	/* background-color: #55ffff; */
	margin: 0 auto;
}
.cen_top>p{
	font-size: 1.25rem;
	line-height: 4.25rem;
}
.cen_top>.p1{
	font-size: 0.8125rem;
	margin-top: -2rem;
}
.echarts_box{
	width: 90%;
	height: 13rem;
	/* background-color: greenyellow; */
	margin: 0 auto;
	margin-top: 2rem;
	margin-bottom: 5.875rem;
}
